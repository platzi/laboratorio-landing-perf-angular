{
  "name": "landing-perf-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "http-server ./dist/landing-perf-angular/browser -p 8080",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/landing-perf-angular/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run landing-perf-angular:serve-ssr",
    "serve:ssr": "node dist/landing-perf-angular/server/main.js",
    "build:ssr": "ng build && ng run landing-perf-angular:server",
    "prerender": "ng run landing-perf-angular:prerender",
    "lhci": "npm run build && lhci autorun --config=lighthouserc.js",
    "lhci:ssr": "npm run build:ssr && lhci autorun --config=lighthouserc-ssr.js",
    "lint": "ng lint",
    "images": "npm run images:resize && npm run images:compress",
    "images:resize": "node scripts/resize.mjs",
    "images:compress": "node scripts/compress.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.5",
    "@angular/common": "14.0.5",
    "@angular/compiler": "14.0.5",
    "@angular/core": "14.0.5",
    "@angular/forms": "14.0.5",
    "@angular/platform-browser": "14.0.5",
    "@angular/platform-browser-dynamic": "14.0.5",
    "@angular/platform-server": "14.0.5",
    "@angular/router": "14.0.5",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@nguniversal/express-engine": "14.0.3",
    "chart.js": "3.8.0",
    "date-fns": "2.28.0",
    "express": "4.15.2",
    "ng2-charts": "3.1.2",
    "rxjs": "7.5.0",
    "swiper": "8.3.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.5",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.0.5",
    "@angular/compiler-cli": "14.0.5",
    "@lhci/cli": "0.9.0",
    "@nguniversal/builders": "14.0.3",
    "@types/express": "4.17.0",
    "@types/jasmine": "4.0.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "8.18.0",
    "http-server": "14.1.1",
    "imagemin": "8.0.1",
    "imagemin-pngquant": "9.0.2",
    "imagemin-webp": "7.0.0",
    "jasmine-core": "4.1.0",
    "karma": "6.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "sharp": "0.30.7",
    "typescript": "4.7.2",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
